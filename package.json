{
  "name": "@sbrighiu/epubjs-rn",
  "version": "0.4.0",
  "repository": "https://github.com/sbrighiu/epubjs-rn",
  "main": "./components/index.js",
  "private": false,
  "scripts": {
    "start": "node ./node_modules/react-native/local-cli/cli.js start",
    "build": "./node_modules/.bin/babel src/ -d components/ --delete-dir-on-start --compact false && cp -R ./src ./components/src/",
    "watch": "./node_modules/.bin/babel src/ -d components/ --compact false -w",
    "local": "./node_modules/.bin/gulp watch",
    "publish-it": "yarn build && npm publish"
  },
  "license": "BSD-2-Clause",
  "dependencies": {
    "@lightbase/react-native-orientation": "latest",
    "@react-native-async-storage/async-storage": "^1.22.0",
    "base-64": "^1.0.0",
    "buffer": "^6.0.3",
    "epubjs": "^0.3.93",
    "event-emitter": "^0.3.5",
    "events": "^3.3.0",
    "i": "^0.3.7",
    "jszip": "^3.10.0",
    "lodash": "^4.18.0",
    "path-webpack": "^0.0.3",
    "react": "18.2.0",
    "react-native": "0.76.2",
    "react-native-static-server": "^0.6.0",
    "react-native-webview": "^12.5.0",
    "react-native-zip-archive": "^6.0.0",
    "react-native-blob-util": "^0.15.0",
    "util": "^0.13.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.0",
    "@babel/core": "^7.26.0",
