    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-static-fs": "^3.0.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "gulp": "^4.0.2",
    "metro-react-native-babel-preset": "^0.79.0"
